{
  "buildCommand": "chmod -R +x node_modules/.bin && npm run build",
  "outputDirectory": "build",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "rewrites": [
    {
      "source": "/public/:path*",
      "destination": "/:path*"
    }
  ]
}
